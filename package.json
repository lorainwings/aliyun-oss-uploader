{
  "name": "@atomfe/oss-uploader",
  "version": "1.1.0",
  "description": "A powerful OSS uploader for uploading files to Aliyun OSS",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "oss-uploader": "./dist/cli.mjs"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "prepare": "husky",
    "prepublishOnly": "pnpm run build && pnpm run lint && pnpm run format:check && pnpm run test",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm run build && changeset publish"
  },
  "keywords": [
    "aliyun",
    "oss",
    "upload",
    "uploader",
    "cli",
    "storage",
    "cloud",
    "deployment",
    "file-upload",
    "batch-upload",
    "aliyun-oss",
    "oss-uploader",
    "oss-cli",
    "typescript"
  ],
  "author": "lorainwings",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lorainwings/aliyun-oss-uploader.git"
  },
  "bugs": {
    "url": "https://github.com/lorainwings/aliyun-oss-uploader/issues"
  },
  "homepage": "https://github.com/lorainwings/aliyun-oss-uploader#readme",
  "packageManager": "pnpm@10.18.2",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "env.example"
  ],
  "dependencies": {
    "ali-oss": "^6.21.0",
    "chalk": "^5.3.0",
    "cli-progress": "^3.12.0",
    "commander": "^12.1.0",
    "cosmiconfig": "^9.0.0",
    "glob": "^11.0.0",
    "ora": "^8.1.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.10",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/js": "^9.17.0",
    "@types/cli-progress": "^3.11.6",
    "@types/node": "^22.10.2",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.13.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "prettier": "^3.4.2",
    "tsup": "^8.5.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.19.1",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
